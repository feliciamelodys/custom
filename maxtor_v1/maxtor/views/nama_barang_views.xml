<odoo>
<!--    Form-->
    <record id="maxtor_nama_barang_view_form" model="ir.ui.view">
        <field name="name">Nama Barang Form</field>
        <field name="model">maxtor.nama.barang</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                            <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

<!--    Tree-->
    <record id="maxtor_nama_barang_view_tree" model="ir.ui.view">
        <field name="name">Nama Barang Tree</field>
        <field name="model">maxtor.nama.barang</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

<!--    Search-->
    <record id="maxtor_nama_barang_view_search" model="ir.ui.view">
        <field name="name">Nama Barang Search</field>
        <field name="model">maxtor.nama.barang</field>
        <field name="arch" type="xml">
            <search>
                <field name="name"/>

            </search>
        </field>
    </record>

<!--    Action-->
    <record id="maxtor_nama_barang_action" model="ir.actions.act_window">
        <field name="name">Nama Barang</field>
        <field name="res_model">maxtor.nama.barang</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>