            <field name="name">ref_propinsi.view.form</field>
            <field name="model">ref_propinsi</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                            <!-- Add your fields here -->
                            <field name="name"/>
                            <field name="singkatan"/>
                            <field name="deskripsi"/>
                        </group>
                        <group>
                          <field name="kota_ids">
                        <tree>
                          <field name="name"/>
                          <field name="singkatan"/>
                        </tree>
                        </field>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
